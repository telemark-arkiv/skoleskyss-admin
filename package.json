    "handlebars": "4.0.11",
    "hapi": "17.4.0",
    "hapi-auth-cookie": "7.0.0",
    "inert": "4.2.1",
    "jsonwebtoken": "8.2.1",
    "simple-encryptor": "1.2.0",
    "tfk-json-to-xlsx": "1.0.11",
    "tfk-saksbehandling-skoleskyss-fara": "4.1.3",
    "tfk-saksbehandling-skoleskyss-nsb": "3.0.15",
    "tfk-saksbehandling-skoleskyss-tbr": "6.0.22",
