  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/skoleskyss-admin.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/skoleskyss-admin/issues"
  },
  "homepage": "https://github.com/telemark/skoleskyss-admin",
  "now": {
    "alias": [
      "admin.skoleskyss.win"
    ],
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.19.1",
